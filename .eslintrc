{
    "rules": {
        "comma-dangle": [
            1,
            "always-multiline"
        ],
        "no-console": [
            1
        ],
        "no-debugger": [
            1
        ],
        "no-dupe-args": [
            2
        ],
        "no-dupe-keys": [
            2
        ],
        "no-duplicate-case": [
            2
        ],
        "valid-typeof": [
            2
        ],
        "no-unexpected-multiline": [
            2
        ],
        "array-callback-return": [
            1
        ],
        "block-scoped-var": [
            1
        ],
        "dot-location": [
            1,
            "property"
        ],
        "dot-notation": [
            1
        ],
        "eqeqeq": [
            1,
            "smart"
        ],
        "indent": [
            1
        ],
        "no-else-return": [
            1
        ],
        "no-redeclare": [
            2
        ],
        "no-return-assign": [
            2
        ],
        "no-self-assign": [
            2
        ],
        "no-self-compare": [
            2
        ],
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "arrow-body-style": [
            1,
            "as-needed"
        ],
        "constructor-super": [
            2
        ],
        "no-this-before-super": [
            2
        ],
        "no-const-assign": [
            2
        ],
        "no-dupe-class-members": [
            2
        ],
        "generator-star-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "no-var": [
            1
        ],
        "prefer-const": [
            1
        ],
        "object-shorthand": [
            1,
            "always"
        ],
        "semi": [
            1,
            "never"
        ]
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended"
}
